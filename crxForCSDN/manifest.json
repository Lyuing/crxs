{
	"manifest_version": 3,
	"name": "processOn",
	"version": "1.1.0",
	"description": "processOn扩展",
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"action": {
		"default_icon": "img/icon.png",
		"default_title": "processOn 扩展下载能力",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.processon.com/*",
				"https://blog.csdn.net/*",
				"https://*.blog.csdn.net/*",
				"http://*/project/ESL_PLAT_BUG/*"
			],
			"js": [
				"js/jquery-1.11.3.min.js",
				"js/content-script.js",
				"js/ESL_bug_list.js"
			],
			"css": [
				"css/custom.css"
			],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"activeTab",
		"contextMenus",
		"cookies",
		"tabs",
		"notifications",
		"webRequest",
		"storage"
	],
	"host_permissions": [
		"https://www.processon.com/*",
		"https://blog.csdn.net/*",
		"https://*.blog.csdn.net/*",
		"http://*/project/ESL_PLAT_BUG/*"
	],
	"web_accessible_resources": [
		{
			"resources": [
				"js/ESL_bug_list.js",
				"css/custom.css"
			],
			"matches": [
				"https://www.processon.com/*",
				"https://blog.csdn.net/*",
				"https://*.blog.csdn.net/*"
			]
		}
	],
	"chrome_url_overrides": {},
	"omnibox": {
		"keyword": "go"
	}
}